ENV=prod
PORT=8000
# Required for LLM features
OPENAI_API_KEY=your-openai-api-key
OPENAI_MODEL=gpt-4.1-nano
FORWARD_URL=https://hooks.zapier.com/hooks/catch/your-id/your-token

# Ngrok (public URL for webhooks)
# Required to enable ngrok
NGROK_AUTHTOKEN=your-ngrok-token
# Optional: reserved domain (requires paid ngrok plan)
NGROK_DOMAIN=your-reserved-domain.example
# Optional: region (us, eu, ap, au, sa, jp, in)
NGROK_REGION=us
# Optional: set to 1 to disable ngrok startup
NGROK_DISABLE=
